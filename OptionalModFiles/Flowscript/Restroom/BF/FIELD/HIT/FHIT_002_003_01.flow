import( "FHIT_002_003_01.msg" );


void CHK_TOILET_MEN_hook()
{

    
    if ( BIT_CHK( 96 ) == 1 )
    {
        SUB_KFEVT_CHK_TOILET_WOMEN();
    }
    else if ( BIT_CHK( 1039 ) == 1 )
    {
        MSG_WND_DSP();
        MSG( MSG_LAST_GREETING_etc );
        MSG_WND_CLS();
    }
    else if ( BIT_CHK( 66 ) == 1 )
    {
        SUB_MoruShowFace();
        MSG_WND_DSP();
        MSG( MSG_TOILET_WOMEN_MORU );
        MSG_WND_CLS();
        SUB_MoruReturnBag();
    }
    else 
    {
        MSG_WND_DSP();
        MSG_MIND( MSG_TOILET_WOMEN, 0 );
        MSG_WND_CLS();
    }

}



void CHK_TOILET_WOMEN_hook()
{
    int var1;
    
    if ( BIT_CHK( 96 ) == 1 )
    {
        SUB_KFEVT_CHK_TOILET_MEN();
    }
    else if ( BIT_CHK( 1039 ) == 1 )
    {
        MSG_WND_DSP();
        MSG( MSG_LAST_GREETING_etc );
        MSG_WND_CLS();
    }
    else 
    {
        var1 = FLD_SCRIPT_READ( 3210, 10, 0 );
        FLD_SCRIPT_READ_SYNC( var1 );
        FLD_SCRIPT_EXEC( var1, 0 );
        FLD_SCRIPT_FREE( var1 );
    }
}

